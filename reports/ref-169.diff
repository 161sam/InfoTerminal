+// 2) Canvas-Stub (für cytoscape & recharts)
+if (typeof HTMLCanvasElement !== 'undefined') {
+  // Avoid "Could not create canvas of type 2d"
+  (HTMLCanvasElement.prototype as any).getContext = function getContext() {
