{
  "slice_name": "table_links",
  "viz_type": "table",
  "datasource": "finance_prices",
  "params": {
    "groupby": ["asset_id", "ts"],
    "adhoc_filters": [],
    "columns": ["asset_id", "ts", "it_url"],
