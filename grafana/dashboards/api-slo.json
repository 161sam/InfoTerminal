{
  "uid": "api-slo",
  "title": "API SLO",
  "schemaVersion": 36,
  "version": 2,
  "panels": [
    {
      "type": "timeseries",
      "title": "Availability (SLO) – search-api",
      "fieldConfig": { "defaults": { "unit": "percentunit", "min": 0, "max": 100 } },
      "targets": [
        {
          "expr": "(sum(rate(http_requests_total{service=\"search-api\",status!~\"5..\"}[5m])) / sum(rate(http_requests_total{service=\"search-api\"}[5m]))) * 100",
          "legendFormat": "availability",
          "datasource": "Prometheus"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Latency p95 – search-api",
      "fieldConfig": { "defaults": { "unit": "s", "min": 0 } },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_server_duration_seconds_bucket{service=\"search-api\"}[5m])) by (service, le))",
          "legendFormat": "p95",
          "datasource": "Prometheus"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Error rate – search-api",
      "fieldConfig": { "defaults": { "unit": "percentunit", "min": 0, "max": 100 } },
      "targets": [
        {
          "expr": "(sum(rate(http_requests_total{service=\"search-api\",status=~\"5..\"}[5m])) / sum(rate(http_requests_total{service=\"search-api\"}[5m]))) * 100",
          "legendFormat": "error%",
          "datasource": "Prometheus"
        }
      ]
    },
